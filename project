int leds[6] = {2, 3, 4, 5, 6, 7};   // LED pins
int delayTime = 400;                // Custom delay (NOT same as class demo)

void setup() {
  for (int i = 0; i < 6; i++) {
    pinMode(leds[i], OUTPUT);
  }
}

void loop() {
  // Turn LEDs on one by one
  for (int i = 0; i < 6; i++) {
    digitalWrite(leds[i], HIGH);
    delay(delayTime);
    digitalWrite(leds[i], LOW);
  }
}